{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@core/*":
      ["src/core/*"],
      "@modules/*":
      ["src/modules/*"],
      "@styles":
      ["src/styles"],
      "@components":
      ["src/core/components"],
      "@lib":
      ["src/core/lib/"],
      "@utils":
      ["src/core/utils"],
      "@img/*":
      ["assets/assets/img/*"],
      "@navigation":
      ["src/navigation"],
      "@redux":
      ["src/redux"],
      "@config":
      ["src/config"],
      "@interfaces/*":
      ["src/core/interfaces"],
    },
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
}
